{
  "name": "simba",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "// Next.js": "-------------- Next.js Commands --------------",
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "// Development DB": "--------- Development Database ---------",
    "dev:db": "docker compose -f docker-compose.dev.yml up -d",
    "dev:db:stop": "docker compose -f docker-compose.dev.yml down",
    "dev:db:logs": "docker compose -f docker-compose.dev.yml logs -f postgres",
    "dev:db:clean": "docker compose -f docker-compose.dev.yml down --volumes",
    "dev:db:reset": "docker compose -f docker-compose.dev.yml down --volumes && docker compose -f docker-compose.dev.yml up -d",
    "// Docker": "-------------- Docker Commands --------------",
    "docker:build": "docker compose build",
    "docker:build:no-cache": "docker compose build --no-cache",
    "docker:up": "docker compose up --build",
    "docker:up:detached": "docker compose up --build -d",
    "docker:down": "docker compose down",
    "docker:clean": "docker compose down --rmi all --volumes --remove-orphans",
    "docker:logs": "docker compose logs -f",
    "docker:shell": "docker compose exec web sh",
    "docker:restart": "docker compose restart web",
    "docker:rebuild": "npm run docker:down && npm run docker:build && npm run docker:up:detached",
    "// Prisma": "-------------- Prisma Commands --------------",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:push:prod": "prisma db push",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:generate": "dotenv -e .env.local -- prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "dotenv -e .env.local -- prisma migrate dev",
    "db:reset": "dotenv -e .env.local -- prisma migrate reset",
    "db:seed": "dotenv -e .env.local -- prisma db seed",
    "db:status": "prisma migrate status",
    "// Combined": "-------------- Combined Commands --------------",
    "dev:full": "npm run dev:db && npm run dev",
    "deploy": "npm run build && npm run db:migrate",
    "docker:migrate": "docker compose exec web npx prisma migrate deploy",
    "docker:full": "npm run docker:build && npm run docker:up:detached && npm run docker:logs"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@paralleldrive/cuid2": "^3.1.0",
    "@prisma/client": "^6.16.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-table": "^8.21.3",
    "@trpc/client": "^11.6.0",
    "@trpc/server": "^11.6.0",
    "@trpc/tanstack-react-query": "^11.6.0",
    "@uidotdev/usehooks": "^2.4.1",
    "better-auth": "^1.3.14",
    "class-variance-authority": "^0.7.1",
    "client-only": "^0.0.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.544.0",
    "next": "15.5.2",
    "next-themes": "^0.4.6",
    "nuqs": "^2.7.1",
    "react": "^19.1.0",
    "react-day-picker": "^9.11.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.63.0",
    "react-phone-number-input": "^3.4.12",
    "recharts": "^2.15.4",
    "server-only": "^0.0.1",
    "sonner": "^2.0.7",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.3.1",
    "xlsx": "^0.18.5",
